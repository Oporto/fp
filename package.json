{
  "name": "final-project",
  "version": "0.0.1",
  "description": "A simple Node app built on Express, instantly up and running.",
  "main": "server.js",
  "engines": {
    "node": "8.x"
  },
  "repository": {
    "url": "https://github.com/Oporto/fp"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "scripts": {
    "start": "node server.js",
    "watch": "parcel watch src/html/login.html",
    "build": "parcel build src/html/login.html"
  },
  "proxy": "http://localhost:3000/",
  "dependencies": {
    "body-parser": "^1.19.0",
    "bulma": "^0.7.5",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "firebase-admin": "^8.6.0",
    "firestore": "^1.1.6",
    "parcel": "^1.12.4",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "tailwindcss": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "babel-preset-nano-react-app": "^0.1.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "parcel-bundler": "^1.12.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "nano-react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragmaFrag": "React.Fragment"
        }
      ]
    ]
  }
}
